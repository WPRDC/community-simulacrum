{
  "name": "community_explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@adobe/react-spectrum": "3.9.0",
    "@react-spectrum/provider": "3.1.3",
    "@react-spectrum/tabs": "3.0.0-beta.0",
    "@react-types/shared": "^3.5.0",
    "@reduxjs/toolkit": "1.5.1",
    "@spectrum-icons/ui": "3.2.0",
    "@spectrum-icons/workflow": "3.2.0",
    "@types/react-measure": "2.0.6",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "file-saver": "2.0.5",
    "fontfaceobserver": "2.1.0",
    "husky": "6.0.0",
    "i18next": "20.1.0",
    "i18next-browser-languagedetector": "6.1.0",
    "mapbox-gl": "2.2.0",
    "maplibre-gl": "^1.14.0",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.0.9",
    "react-i18next": "11.8.12",
    "react-measure": "2.5.2",
    "react-redux": "7.2.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "4.3.0",
    "react-test-renderer": "17.0.2",
    "react-vega": "7.4.2",
    "recharts": "2.0.9",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "12.0.1",
    "styled-components": "5.2.3",
    "vega": "5.20.2",
    "vega-embed": "6.17.0",
    "vega-lite": "5.0.0",
    "wprdc-components": "../../wprdc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.22",
    "@types/lodash": "4.14.168",
    "@types/mustache": "4.1.1",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-select": "4.0.13",
    "@types/react-test-renderer": "17.0.1",
    "@types/recharts": "1.8.19",
    "@types/styled-components": "5.1.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/webpack-env": "1.16.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "stylelint": "13.12.0",
    "stylelint-config-recommended": "4.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
